{
  "compilerOptions": {
    "target": "ES2019",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "ES2020",
    "baseUrl": "./src/",
    "paths": {
      "@assets/*": [
        "assets/*"
      ],
      "@fonts/*": [
        "assets/fonts/*"
      ],
      "@images/*": [
        "assets/images/*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@routes/*": [
        "routes/*"
      ]
    },
    "outDir": "../public/assets/",
    "jsx": "react-jsx",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": false
  }
}
